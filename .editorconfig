# editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
insert_final_newline = true

[*.js]
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[*.{c,h}]
trim_trailing_whitespace = true
indent_style = tab
indent_size = 8

[Makefile]
trim_trailing_whitespace = true
indent_style = tab
indent_size = 8
